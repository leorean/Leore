            <kind>1</kind>
            <string>// DRAWING &lt;-&gt; STATE LOGIC
if (global.state == G.PLAY
    || state == S.ACQUIRE) {
    switch(state) {
        case S.IDLE:
            fCur = (fCur + .1) % 4;
            frame = floor(fCur);
        break;
        case S.WALK:
            fCur = (fCur + .2) % 4;
            frame = 4 + floor(fCur);
        break;
        case S.ROLL:
            fCur = (fCur + max(.2, .6 * sqrt(abs(xVel) + abs(yVel)) / 3 )) % 4;
            frame = 8 + floor(fCur);
        break;
        case S.FALL:
            frame = 0;
        break;
        case S.ATTACK:
            fCur = 0;
            frame = 12 + floor(fCur);        
        break;
        case S.DROWN:
            fCur = min(fCur + .1, 4);
            frame = floor(fCur);
            if (fCur == 4) {
                if (inWater) {
                    x = xPrevDrown;
                    y = yPrevDrown;
                } else {
                    state = S.IDLE;
                }
            }
        break;
        case S.SWIM:
            fCur = (fCur + .1) % 3;
            frame = 13 + floor(fCur);        
        break;
        case S.ACQUIRE:
            fCur = 0;
            frame = 16;
        break;
    }
}

switch(dir) {
    case D.UP:
        row = 1;
    break;
    case D.DOWN:
        row = 0;
    break;
    case D.LEFT:
        row = 3;
    break;
    case D.RIGHT:
        row = 2;
    break;
}

// DRAWING 

// shadow
if (!inShallowWater &amp;&amp; !inWater &amp;&amp; !inHole)
{
    draw_sprite(sprShadow, -1, x, bbox_bottom);
}

if (!inHole &amp;&amp; state != S.DROWN) {
    if (((invincible + hurt) % 4) &lt; 2) {
    } else {
        d3d_set_fog(1, c_black, 0, 0)   
    }
    draw_sprite_part(sprPlayer, -1, frame*32, row * 32, 32, 32, x - 16, cy - 18 + z);
    d3d_set_fog(0,0,0,0)
}
else {
    var s_fall = (fallTimeout / maxFallTimeout);
    draw_sprite_ext(sprPlayerFall, -1, cx, cy, s_fall, s_fall, s_fall * 360, c_white, s_fall);
}

// shallow water
if (inShallowWater &amp;&amp; !inWater) {
    draw_sprite_part(sprWaterShadow, -1, 32*floor((global.timer / 5) % 4), 0, 32, 32, x - 16, y - 20);
}

if (state == S.DROWN) {
    draw_sprite(sprDrown, frame, cx, cy);
}

if (global.debug) {
    draw_rectangle(bbox_left, bbox_top, bbox_right, bbox_bottom, true);
    draw_circle(cx, cy, 6, 1);
}
</string>
          </argument>
        </arguments>
      </action>
