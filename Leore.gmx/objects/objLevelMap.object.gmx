        <arguments>
          <argument>
            <kind>1</kind>
            <string>if (sprMap = noone) {

    var surf = surface_create(w, h);
    surface_set_target(surf);
    
    for (var i = 0; i &lt; w; i++) {
        for (var j = 0; j &lt; h; j++) {
            val = ds_grid_get(map, i, j);
            col = c_white;
            switch(val) {
                default: // nothing
                    //col = make_color_hsv(0, 0, 0);
                    draw_set_alpha(0);
                break;
                case 1: // solid
                    //col = make_color_hsv(0, 0, 255);
                    draw_set_alpha(1);
                break;
                case 2: // deepwater
                    //col = make_color_hsv(255, 255, 190);
                    draw_set_alpha(.4);
                break;
                case 3: // shallow water
                    //col = make_color_hsv(255, 255, 255);
                    draw_set_alpha(.6);
                break;
                case 4: // floor
                    //col = make_color_hsv(0, 0, 220);
                    draw_set_alpha(.8);
                break;
            }
            if (val) {
                draw_set_color(col);
                draw_point(i, j);
            }
        }
    }    
    sprMap = sprite_create_from_surface(surf, 0, 0, w, h, 0, 0, 0, 0);
    
    surface_reset_target();
    surface_free(surf);
}

if (global.state != G_PLAY &amp;&amp; global.state != G_PAUSE) exit;

xo = view_wview - sprite_get_width(sprMap) - 3;
yo = view_hview - sprite_get_height(sprMap) - 3;

// DRAW BG
draw_set_alpha(1);
draw_sprite_ext(sprMap, -1, xo, yo, 1, 1, 0, c_white, .75);

// DRAW PLAYER
var px = objPlayer.x / room_width;
var py = objPlayer.y / room_height;
if ((global.timer % 10) &gt; 5) {
    draw_sprite_ext(mskPixel, -1, xo + px * sprite_get_width(sprMap) , yo + py * sprite_get_height(sprMap), 1, 1, 0, c_white, 1);
}

// DRAW ENEMIES
for (var i = 0; i &lt; ds_list_size(enemies); i++) {
    var e = ds_list_find_value(enemies, i);
    if (instance_exists(e)) {
        px = e.x / room_width;
        py = e.y / room_height;
        draw_sprite_ext(mskPixel, -1, xo + px * sprite_get_width(sprMap) , yo + py * sprite_get_height(sprMap), 1, 1, 0, c_red, 1);
    }
}
</string>
