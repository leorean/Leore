inWater = false;
inCliff = false;

fallTimeout = 0;
maxFallTimeout = 60;
maxCliffTimer = 30;
cliffTimer = 0;

state = S.IDLE;
fCur = 0;
frame = 0;
dir = global.dir;
angle = 270;
row = 0;

sword = instance_create(x, y, objSword);
hp = 3;
hurt = 0;
