            <kind>1</kind>
            <string>kUp = keyboard_check(vk_up);
kDown = keyboard_check(vk_down);
kLeft = keyboard_check(vk_left);
kRight = keyboard_check(vk_right);

kUp = (kUp &amp;&amp; !kDown);
kDown = (!kUp &amp;&amp; kDown);
kLeft = (kLeft &amp;&amp; !kRight);
kRight = (!kLeft &amp;&amp; kRight);

kRoll = keyboard_check(ord('A'));
</string>
