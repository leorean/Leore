        <arguments>
          <argument>
            <kind>1</kind>
            <string>if (keyboard_check_pressed(vk_space)) { zVel = -2; }
if (keyboard_check_pressed(ord('A'))) { vel = 10; }

if(drown) {
    drownTimer = max(drownTimer - 1, 0);
    if (!drownTimer) {
    
        if (inWater) {
            x = xPrev;
            y = yPrev;
        }
    
        drown = false;
    }
}

if (z == 0 &amp;&amp; !isHold &amp;&amp; mouse_check_button(mb_left)) {
    if (vel &gt; .5)
        vel *= .95;
    else
        vel = 0;
}

canDestroy = (!isHold &amp;&amp; !inWater);

if (!inWater &amp;&amp; !drown &amp;&amp; state == IDLE) {
    if (!isHold &amp;&amp; mouse_check_button_pressed(mb_left)) {
        mouse_clear(mb_left);
        mx[0] = device_mouse_x_to_gui(0);
        my[0] = device_mouse_y_to_gui(0);
        isHold = true;
        state = ROLL;
        xPrev = x;
        yPrev = y;
    }
}
if (isHold) {
    mx[1] = device_mouse_x_to_gui(0);
    my[1] = device_mouse_y_to_gui(0);
    angle = point_direction(mx[0], my[0], mx[1], my[1]);
    vel = min(point_distance(mx[0], my[0], mx[1], my[1]) / 64, maxVel);

    if (mouse_check_button_released(mb_left)) {
    
        xVel = lengthdir_x(vel, angle);
        yVel = lengthdir_y(vel, angle);             
        isHold = false;
    }
}

if (state == ROLL) {
    if (vel &lt; .1) 
    {
        if (!isHold &amp;&amp; z == 0) {
            state = IDLE;
        }
    } else {
        fSpeed = .6 * vel / maxVel;
    }
}

if (vel &gt; .2) {
    if (abs(lengthdir_x(1, angle)) &gt; abs(lengthdir_y(1, angle))) {
        dir = sign(lengthdir_x(1, angle));
    } else {
        dir = 2*sign(lengthdir_y(1, angle));
    }
} else
{
    vel = 0;
}

fSpeed = max(.1, .6 * vel / maxVel);

if (!isHold) {
    vel *= .985;
}

inWater = z == 0 &amp;&amp; tile_layer_find(LAYER_WATER, x, y);

if (inWater) {
    isHold = false;
    vel *= .93;
    
    if (vel &lt; .3) {
        // Drown!
        if (!drown) {
            drown = true;
            drownTimer = maxDrownTimer;
            vel = 0;
        }
    }
}


zVel += zGrav;
z += zVel;
if (z + zVel &gt;= 0) {
    zVel = -zVel * .5;
}
z = min(z, 0);

instance_deactivate_object(objDestroyable);
instance_activate_region(bbox_left - 8, bbox_top - 8, 24, 24, true);
for(var i = 0; i &lt; instance_number(objDestroyable); i++) {

    var d = instance_find(objDestroyable, i);
    var destroyable = instance_place(x + lengthdir_x(vel + 1, angle), y + lengthdir_y(vel + 1, angle), d);
    var newAngle = angle;
    
    if (instance_exists(destroyable)) {
        if (place_meeting(x + lengthdir_x(vel, angle), y, destroyable)) {
            newAngle = point_direction(0, 0, -lengthdir_x(vel, angle), lengthdir_y(vel, angle));
        }
        else if (place_meeting(x, y + lengthdir_y(vel, angle), destroyable)) {
            newAngle = point_direction(0, 0, lengthdir_x(vel, angle), -lengthdir_y(vel, angle));
        } else
        {
            destroyable = noone;
        }
    }    
    if (destroyable != noone) {
        if (destroyable.hp &gt; 0) {
            if (canDestroy) {
                destroyable.hp = max(destroyable.hp - 1, 0);
                show_debug_message(string(destroyable) + ": " + string(destroyable.hp));
            }
            angle = newAngle;
        }
    }
}
instance_activate_object(objDestroyable);

// collision
if (!isHold) {
    if (!place_meeting(x + lengthdir_x(vel, angle), y, objSolid)) {
        x += lengthdir_x(vel, angle);
    } else {
        angle = point_direction(0, 0, -lengthdir_x(vel, angle), lengthdir_y(vel, angle));
    }
    if (!place_meeting(x, y + lengthdir_y(vel, angle), objSolid)) {
        y += lengthdir_y(vel, angle);    
    } else {
        angle = point_direction(0, 0, lengthdir_x(vel, angle), -lengthdir_y(vel, angle));
    }
}
</string>
